print ('Введите число "a"')
a = int(input())
print ('Введите число "b"')
b = int(input())
print ('Введите число "c"')
c = int(input())
if a + b == c:
    print('"a" и "b" в сумме дают "c"')
else:
    print('"a" и "b" в сумме не дают "c"')
if a / b == c:
    print('"a" разделить на "b" равно "c"')
else:
    print('"a" разделить на "b" не равно "c"')
